{
  "name": "digivore-frontend",
  "version": "0.0.0",
  "description": "Digivore frontend",
  "repository": "https://github.com/ezgraf/digivore",
  "main": "./main.js",
  "keywords": [],
  "contributors": [
    {
      "name": "ezgraf",
      "email": "ezgraf@digivore.fr",
      "homepage": "https://github.com/ezgraf"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "typescript-dev-kit": "^3.0.5"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/typescript-dev-kit/main.js"
    ]
  },
  "dependencies": {
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "basx": "^1.3.3",
    "diox": "^4.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "tsDevKitConfig": {
    "target": "web",
    "entry": {
      "main": "./scripts/main.tsx"
    },
    "devServer": {
      "ip": "0.0.0.0",
      "port": 5111
    },
    "splitChunks": true,
    "srcPath": "src",
    "distPath": "public",
    "banner": "/*!\n * Copyright (c) ...\n * All rights reserved.\n */",
    "env": {
      "development": {
        "NODE_ENV": "development",
        "API_URL": "http://localhost:9000/api"
      },
      "production": {
        "NODE_ENV": "production",
        "API_URL": "/api"
      }
    }
  },
  "scripts": {
    "init": "node_modules/typescript-dev-kit/scripts/init.sh",
    "dev": "cd node_modules/typescript-dev-kit/ && node scripts/dev",
    "build": "cd node_modules/typescript-dev-kit/ && node scripts/build",
    "test": "cd node_modules/typescript-dev-kit/ && node scripts/test",
    "doc": "node_modules/.bin/typedoc --out ./doc/ --exclude \"**/*.js\" --exclude \"**/__+(tests|mocks)__/**\" src/",
    "postinstall": "rm -f node_modules/.eslintcache"
  }
}